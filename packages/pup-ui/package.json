{
	"name": "pup-ui",
	"version": "0.0.4",
	"description": "pup-ui",
	"main": "index.js",
	"module": "./dist/pup-ui.esm.js",
	"scripts": {
		"dev": "vite",
		"build": "pnpm build:components",
		"build:all": "vite build -c ../../vite.config.ts",
		"build:components": "esno ./scripts/build.ts",
		"test": "vitest",
		"lint": "eslint --fix --ext .ts,.vue src",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\"",
		"test:run": "vitest run",
		"coverage": "vitest run --coverage"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yxw007/Pup-ui.git"
	},
	"author": "Potter",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/yxw007/Pup-ui/issues"
	},
	"homepage": "https://pup-ui.vercel.app/",
	"devDependencies": {
		"@iconify-json/ic": "1.1.4",
		"@types/fs-extra": "^9.0.13",
		"@unocss/preset-attributify": "0.35.1",
		"@unocss/preset-icons": "0.30.10",
		"@unocss/preset-uno": "0.35.1",
		"@vitejs/plugin-vue": "3.0.3",
		"@vue/test-utils": "2.0.2",
		"c8": "^7.12.0",
		"esno": "^0.16.3",
		"fs-extra": "^10.1.0",
		"happy-dom": "6.0.4",
		"terser": "5.4.0",
		"ts-node": "^10.9.1",
		"vitest": "^0.14.0",
		"unocss": "0.33.4"
	},
	"dependencies": {
		"@testing-library/vue": "^6.6.1",
		"vue": "^3.2.40"
	}
